{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
		"moduleResolution": "Node"
  },
	"include": ["src/**/*"]
}
